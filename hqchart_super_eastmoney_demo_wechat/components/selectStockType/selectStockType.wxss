/* components/selectStockType/selectStockType.wxss */
.box{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  background-color: #fff;
}

.stockList{
  flex: 1;
  overflow: hidden;
}

.item{
  height: 100rpx;
  border-bottom: 1px solid #edeff5;
  display: flex;
  align-items: center;
  padding-left: 30rpx;
}

.code {
  font-size: 32rpx;
	color: #868d9f;
  padding-right: 32rpx;
}

.name{
  font-size: 32rpx;
}
